[sub_resource type="GDScript" id="GDScript_lrv8e"]
script/source = "extends SwitchComponent

func on_interaction(requested:bool):
	if requested == is_activated:
		return
	if requested:
		open_door()
	else:
		close_door()

func open_door():
	is_activated = true
	$AnimationPlayer.play(\"Opening\")
	
func close_door():
	is_activated = false
	$AnimationPlayer.play(\"Closing\")
"

